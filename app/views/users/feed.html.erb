<% if user_signed_in? %>
  <%= "@" + current_user.name %></br>
  <% @feed_posts.each do |post| %>
    <%= post.user.name %></br>
    <%= post.desc %></br>
    <%= image_tag post.images.url  %>
  <% end %>
  <%= link_to "sign out", destroy_user_session_path, method: "DELETE" %></br>

  <%= current_user.id %> 
<% end %>
